<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <!-- 키 -->
    <entry key="selectEmpByFamilyName">
        <!-- 밸류 -->
        select *
        from employee
        where emp_name like concat(?, '%')
    </entry>
</properties>